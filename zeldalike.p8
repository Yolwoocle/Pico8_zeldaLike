function _init()
	actors = {}
	init_player()
	create_room(r2)
	mx=0
	my=0
end

function _update60()
	mouse_x_y()
	player_update()
	for i in all(players) do
	 if (stat(34) & 1==1) spawn_bullet(i.x,i.y,1,1,10,1,60)
	 if (stat(34) & 2==2) spawn_bullet(i.x,i.y,2,1,5,6,10,10)
	end
	
	for a in all(actors) do
		a:update()
		if(a.destroy_flag)del(actors,a)
	end
end

