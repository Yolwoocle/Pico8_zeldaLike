--gun & bullet

function make_gun(name,spr,cd,spd,oa,fire)
	local gun = {
		name=name,
		spr=64,
		bulletspd=spd,
		oa=oa,--offset angle in [0,1[
		
		timer=0,
		cooldown=cd,
	}
	
	gun.fire = fire
	
	gun.shoot=function(gun,x,y,dir)
		if gun.timer<=0 then
			spawn_bullet(x,y,dir,
			gun.bulletspd,80)
			
			gun.timer = gun.cooldown
		end
	end
	
	gun.update=function(gun)
		gun.timer = max(gun.timer-1,0)
	end
	
	return gun
end

guns = {
	revolver = make_gun("revolver",
--spr cd spd oa
	 64, 10,4, .02,
	 function(gun,x,y,dir)
	 	dir+=rnd(2*gun.oa)-gun.oa
	 	gun:shoot(x,y,dir)
	 end),
	
	shotgun = make_gun("shotgun",
--spr cd spd oa
	 65, 20,20, .05,
	 function(gun,x,y,dir)
	 	for i=1,5 do
	 		gun:shoot(x,y,dir)
	 	end
	 end),
}

function spawn_bullet(x,y,dir,spd,spr)
	local dx=cos(dir)*spd
	local dy=sin(dir)*spd
	
	add(actors,{
		x=x,  y=y,
		dx=dx,dy=dy,
		
		spr=spr,
		
		destroy_flag=false,
		update=update_bullet,
		draw=draw_bullet,
	})
end

function update_bullet(b)
	b.x += b.dx
	b.y += b.dy
	
	if is_solid(b.x,b.y) then
		b.destroy_flag = true
	end
end

function draw_bullet(b)
	spr(b.spr, b.x-4, b.y-4)
end

