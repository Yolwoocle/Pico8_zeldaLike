--enemies
function make_enemy(x,y,spr,spd,gunt)
	return {
		x=x, y=y,
		angle=0,
		dx=0,dy=0,
		spd=spd,
		
		bx=1,by=1,
		bw=6,bh=6,
		
		spr=spr,
		life=10,
		
		gun=gunt,
		cd=30,
		timer = 0,
		a=0,
	}
end

function init_enemies()
	enemies = {}
	checker = {}
end

function spenemie(x,y)
	add(enemies,make_enemy(
	 x,y,
	 96,1,
	 copy(guns.enemy_revolver)
	))
end

function update_enemy(e)
	for i in all(enemies) do 
	if abs(players[1].x-e.x)<70 then
		changedirection(i)
		collide(i,1)
		i.x += i.dx/#enemies
		i.y += i.dy/#enemies

		i.gun.timer = max(i.gun.timer-1/#enemies,0)
		if i.gun.timer<=0 and 
		canshoot(i) then
			i.gun:fire(i.x+4,i.y+4,i.a)
		end
	end
	end
end

function draw_enemy(e)
	spr(e.spr, e.x,e.y)
	print(e.life, e.x,e.y-8,7)
	--print(e.gun.timer,e.x,e.y)
	--print(e.dy,e.x,e.y+6)
end

function changedirection(i)
	i.timer-=1
	if i.timer < 1 then
	 i.angle += rnd(0.5)-0.25
	 i.timer=i.cd
	 i.dx=cos(i.angle)/8--/(#enemies)*i.spd 
	 i.dy=sin(i.angle)/8--/(#enemies)*i.spd
	end
end

function canshoot(e)
	local angle = atan2(players[1].x-e.x,
	players[1].y-e.y)
	e.a=angle
	local x = cos(angle)
	local y = sin(angle)
	local dist =sqrt(abs(players[1].y-e.y)^2+abs(players[1].x-e.x)^2)/8
	if abs(dist)<7 and abs(players[1].x-e.x)<128 then
	 for i =1,dist do
	 add(checker,{x=e.x+x*i*8,y=e.y+y*i*8})  
	  if is_solid(checker[#checker].x+4,checker[#checker].y+4) then
	 	 delchecker()
	 	 e.gun.timer = e.gun.cooldown
	   return false 
	  end
	 end
	 delchecker()
	 return true 
	end
end

function drawcheck()
	for i in all(checker) do
	 spr(19,i.x,i.y)
	end
end

function delchecker()
	checker = {}
end
