--player
function init_player()
	players={}
	add(players, 
	{
		n=1,
		
		x=64,y=64,
		dx=0,dy=0,
		a=0,
		
		spd=.4,
		fric=0.75,
		
		bx=2,by=2,
		bw=4,bh=4,
		
		spr=112+rnd(12),
		
		gun=copy(guns.revolver),
	})
end

function player_update()
	for p in all(players) do
		--movement
		local dx,dy = p.dx,p.dy
		local spd = p.spd
		
		if (btn(⬅️,p.n)) p.dx-=spd
		if (btn(➡️,p.n)) p.dx+=spd
		if (btn(⬆️,p.n)) p.dy-=spd
		if (btn(⬇️,p.n)) p.dy+=spd
		
		p.dx *= p.fric
		p.dy *= p.fric
		
		collide(p,0.1)
		
		p.x += p.dx
		p.y += p.dy
		
		--angle
		p.a = atan2(mouse_x-p.x,
		mouse_y-p.y)
		
		p.flip=false
		if(p.a<.75and.25<p.a)p.flip = true
		
		--shooting
		local fire=stat(34)&1 > 0
		local active=stat(34)&2 > 0
		
		p.gun:update()
		if fire and p.gun.timer<=0 then
			p.gun:fire(p.x+4,p.y+4,p.a)
		end
		
		--next wagon
		if p.x>128*wagonlen then
			wagon_n += 1
			update_room()
			
			--pan cam to next wagon
			camx = -128
			targetcamx=-8
			for p in all(players)do
				p.x -= 128*wagonlen
			end
		end
	end
end

function draw_player()
	for p in all(players) do
		local x=flr(p.x) + cos(p.a)*6 +0
		local y=flr(p.y) + sin(p.a)*3 +0
		spr(p.gun.spr,x,y,1,1, p.flip)
		print(cos(p.a),2,2,7)
		print(p.a,2,10,7)
		spr(p.spr,p.x,p.y,1,1, p.flip)
	end
end

