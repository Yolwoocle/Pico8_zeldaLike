function create_room(room)
	local xroom = 0
	local yroom = 0
	local number = 0
	for i in all(room) do
		mset(xroom,yroom,i)
		xroom +=1
		number += 1
		if number == 15 then
		number = 0
		yroom += 1
		xroom = 0
		end
	end

end




