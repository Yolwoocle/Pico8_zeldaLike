--map 
test = 0

function copy_room(co_x,co_y)
	co_x = co_x*16
	room_all= {}
	for j = 0,15 do
		for i=0,15 do
			add(room_all,mget(i+co_x,16+j))
		end
	end
end

function create_room(number)
	number = number*15
	local cx = 0
	local cy = 0
	for sprite in all(room_all) do
		mset(cx+number,0+cy,sprite)
		cx += 1
		if cx%16 == 0 then
			cx = cx%16
			cy += 1
		end
	end
end

function update_room()
	rr = rnd({0,1})
	if players[1].x > 304 then
		players[1].x = 64
	
	copy_room(1,co_y)
	create_room(0)
	
	copy_room(rr,co_y)
	create_room(1)
	
	copy_room(1,co_y)
	create_room(2)
	
	copy_room(rr,co_y)
	create_room(3)
	end
end
