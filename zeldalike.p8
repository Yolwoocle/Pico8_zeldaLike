--player
function init_player()
	players={}
	add(players, 
	{
		n=1,
		
		x=64,y=64,
		dx=0,dy=0,
		a=0,
		
		spd=.5,
		fric=0.8,
		
		bx=0,by=0,
		bw=8,bh=8,
		
		gun=make_gun(64,10,3),
	})
end

function player_update()
	for p in all(players) do
	 --movement
	 local dx,dy = p.dx,p.dy -- oui ?
	 local spd = p.spd -- ?
	 
	 if (btn(⬅️,p.n)) p.dx-=spd
	 if (btn(➡️,p.n)) p.dx+=spd
	 if (btn(⬆️,p.n)) p.dy-=spd
	 if (btn(⬇️,p.n)) p.dy+=spd
	 
	 p.dx *= p.fric
	 p.dy *= p.fric
	 
	 collide(p)
	 
	 p.x += p.dx
	 p.y += p.dy
	 
	 --angle
		p.a = atan2(mouse_x-p.x,
		mouse_y-p.y)
		
		--shooting
		local fire=stat(34)&1 > 0
		local active=stat(34)&2 > 0
		
		if fire then
			p.gun.shoot(p.x+4,p.y+4,p.a)
		end
	end
end

function draw_player()
	for p in all(players) do
		spr(112,p.x,p.y)
		spr(64,p.x+5,p.y-7)
	end
end

