--gun & bullet

function make_gun(name,spr,cd,spd,oa,is_enemy,fire)
	local gun = {
		name=name,
		spr=spr,
		spd=spd,
		oa=oa,--offset angle in [0,1[
		
		timer=0,
		cooldown=cd,
		is_enemy=is_enemy
	}
	
	gun.fire = fire
	
	gun.shoot=function(gun,x,y,dir,spd)
		--remove? it complicates code
		local s=80
		if(gun.is_enemy)s=82
		
		spd = spd or gun.spd
		spawn_bullet(x,y,dir,
		spd,3,s,is_enemy)
		gun.timer = gun.cooldown
	end
	
	gun.update=function(gun)
		gun.timer = max(gun.timer-1,0)
	end
	
	return gun
end

guns = {
	revolver = make_gun("revolver",
--spr cd spd oa is_enemy
		64, 10,3, .02,false,
		function(gun,x,y,dir)
			dir+=rnd(2*gun.oa)-gun.oa
			gun:shoot(x,y,dir)
		end
	),
	
	shotgun = make_gun("shotgun",
--spr cd spd oa is_enemy
	 65, 60,4, .05,false,
	 function(gun,x,y,dir)
	 	for i=1,8 do
	 		local o=rnd(.1)-.05
	 		local ospd=gun.spd*(rnd(.2)+.9)
	 		gun:shoot(x,y,dir+o, ospd)
	 	end
	 end),
	 
	 
	enemy_revolver = make_gun("revolver",
--spr cd spd oa is_enemy
		64, 100,1, .02,true   ,
		function(gun,x,y,dir)
			dir+=rnd(2*gun.oa)-gun.oa
			gun:shoot(x,y,dir)
		end
	,true),
}

function spawn_bullet(x,y,dir,spd,r,spr,is_enemy)
	local dx=cos(dir)*spd
	local dy=sin(dir)*spd
	add(actors,{
		x=x,  y=y,
		dx=dx,dy=dy,
		r=r,
		
		spr=spr,
		is_enemy=is_enemy,
		destroy_flag=false,
		update=update_bullet,
		draw=draw_bullet,
	})
end

function update_bullet(b)
	b.x += b.dx
	b.y += b.dy
	
	for e in all(enemy)do
		local a1={b.x-b.r, b.y-b.r}
		local a2={b.x+b.r, b.y+b.r}
		
		local b1={e.x, e.y}
		local b2={e.x+e.bx+e.bw, 
		          e.y+e.dy+e.bh}
		if rect_intesect(a1,a2,b1,b2)then
			e.life -= 1--b.dmg
			b.destroy_flag = true
		end
	end
	
	if is_solid(b.x,b.y) then
		if check_flag(1,b.x,b.y) then
		 swichtile(b.x,b.y)
		end
		b.destroy_flag = true
	end
end

function draw_bullet(b)
	spr(b.spr, b.x-4, b.y-4)
end

