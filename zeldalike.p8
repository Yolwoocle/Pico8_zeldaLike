function _init()
	actors = {}
	init_player()
	--create_room(r1)
	init_enemies()
	--create_room(r2)
	mx=0
	my=0
	copy_room(1,co_y)
	create_room(0)
	copy_room(0,co_y)
	create_room(1)
	copy_room(1,co_y)
	create_room(2)
	copy_room(0,co_y)
	create_room(3)
end

function _update60()
	update_room()	
	mouse_x_y()
	player_update()
	for i in all(players) do
	end
	
	for a in all(actors) do
		a:update()
		if(a.destroy_flag)del(actors,a)
	end
	
	for e in all(enemies) do
		update_enemy(e)
		if(e.destroy_flag)del(enemy,e)
	end
end

function _draw()
	pal(1,129,1)
	camera(players[1].x-60)


	cls()
	map()
	
	--darker blue
	pal(1,129,1)
	poke(0x5f2e,1)
	
	draw_player()
	--draw_room()
	check_flag(1,mouse_x,mouse_y)
	print(players[1].x,players[1].x,0)
	for a in all(actors) do
		a:draw()
	end
	
	for e in all(enemies) do
		draw_enemy(e)
	end
	
	draw_mouse()
end
