p = {}
function _update60()
	add(p,{x=rnd(128),y=128,
	r=rnd(30)+20,dx=rnd(10)-5,
	dy=rnd(10)-5,
	})
	
	for i in all(p)do
		i.x += i.dx
	end
end

function _draw()
	cls(15)
	draw_logo(44,35)
	
	pal(1,129,1)
end

function draw_logo(x,y)
	--"birds"
	oxxl("birds",cos(time())*2.9+44,
	sin(t())*2.9+35,10)
	oxxl("guns",
	cos(t()+.2)*2.9+44+4,
	sin(-t()+.2)*2.9+35+15, 6)
	
	--"with"
	oprint("with",
	cos(t()+.7)*.9+44+11,
	sin(t()+.7)*.9+35+10)
	
	oprint("with",
	cos(t()+.7)*.9+44+11,
	sin(t()+.7)*.9+35+9)
	
end
